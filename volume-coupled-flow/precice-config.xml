<?xml version="1.0"?>
<precice-configuration>
  <log>
    <sink
      filter="%Severity% > debug and %Rank% = 0"
      format="---[precice] %ColorizedSeverity% %Message%"
      enabled="true" />
  </log>

  <data:vector name="Velocity" />

  <mesh name="Fluid-Mesh">
    <use-data name="Velocity" />
  </mesh>

  <mesh name="Dummy-Mesh">
    <use-data name="Velocity" />
  </mesh>

  <participant name="Fluid">
    <provide-mesh name="Fluid-Mesh" />
    <receive-mesh name="Dummy-Mesh" from="Dummy-Velocity" />
    <mapping:nearest-neighbor
      direction="read"
      from="Dummy-Mesh"
      to="Fluid-Mesh"
      constraint="consistent" />
    <read-data name="Velocity" mesh="Fluid-Mesh" />
  </participant>

  <participant name="Dummy-Velocity">
    <provide-mesh name="Dummy-Mesh" />
    <write-data name="Velocity" mesh="Dummy-Mesh" />
  </participant>

  <m2n:sockets acceptor="Dummy-Velocity" connector="Fluid" exchange-directory=".." />

  <coupling-scheme:serial-explicit>
    <time-window-size value="0.005" />
    <max-time value="4.0" />
    <participants first="Dummy-Velocity" second="Fluid" />
    <exchange data="Velocity" mesh="Dummy-Mesh" from="Dummy-Velocity" to="Fluid" />
  </coupling-scheme:serial-explicit>
</precice-configuration>
