FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    interface
    {
        type            solidForce;
        forceField      solidForce;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


