      "decomposition": [2, 1],
      "adaptivity": {
        "type": "global",
        "data": ["k_00", "k_11", "porosity", "concentration"],
        "history_param": 0.1,
        "coarsening_constant": 0.2,
        "refining_constant": 0.05,
        "every_implicit_iteration": "False",
        "similarity_measure": "L2rel"
      }
    },
    "diagnostics": {
      "data_from_micro_sims": {"grain_size": "scalar"}
    }
}
