(
    ($x0 $y0 $w_f)        // 0
    ($x1 $y0 $w_f)        // 1
    ($x2 $y0 $w_f)        // 2
    ($x3 $y0 $w_f)        // 3
    ($x0 $y1 $w_f)        // 4
    ($x1 $y1 $w_f)        // 5
    ($x2 $y1 $w_f)        // 6
