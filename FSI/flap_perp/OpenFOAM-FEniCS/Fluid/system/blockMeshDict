    ($x0 $y2 $w_f)        // 8
    ($x1 $y2 $w_f)        // 9
    ($x2 $y2 $w_f)        // 10
    ($x3 $y2 $w_f)        // 11
         
    ($x0 $y0 $w_b)         // 12
    ($x1 $y0 $w_b)         // 13
    ($x2 $y0 $w_b)         // 14
    ($x3 $y0 $w_b)         // 15
    ($x0 $y1 $w_b)         // 16
    ($x1 $y1 $w_b)         // 17
    ($x2 $y1 $w_b)         // 18
    ($x3 $y1 $w_b)         // 19
    ($x0 $y2 $w_b)         // 20
    ($x1 $y2 $w_b)         // 21
    ($x2 $y2 $w_b)         // 22
    ($x3 $y2 $w_b)         // 23
);

