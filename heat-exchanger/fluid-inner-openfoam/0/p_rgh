FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/Zone_solid_2";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    outlet
    {
        type            fixedValue;
        value           uniform 101325;
    }
    inlet
    {
        type            zeroGradient;
    }
    interface
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 101325;
    }
    adiabatic
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 101325;
    }
}


