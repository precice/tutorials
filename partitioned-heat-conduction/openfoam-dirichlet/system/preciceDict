      Temperature
    );
    
    writeData
    (
      Heat-Flux
    );
  };
};

CHT
{
   // fenics and nutils use different
   // sign conventions here, so we
   // select -1 for compatibility
   // reasons
   k   [ 1  1 -3 -1 0 0 0 ] -1;
   solverType "basic";
};
